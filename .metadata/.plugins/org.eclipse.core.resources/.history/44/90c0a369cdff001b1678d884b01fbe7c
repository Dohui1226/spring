<mapper namespace="board.BoardDAO">
<insert id="newBoard" parameterType="kr.ac.kopo.board.BoardVO">
	insert into t_board(no,title,writer,content)
	values(seq_t_board_no.nextval, '${title}', '${writer}','${content}')
</insert>
</mapper>